{
  "version": 2,
  "name": "react-hook-form",
  "alias": "react-hook-form",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "status": 301, "headers": { "Location": "https://react-hook-form.com/$1" }}
  ]
}
